# Required for production: GitHub Pages origin (no trailing slash)
ORIGIN_PAGES=https://<your-user>.github.io

# Optional additional origins (comma-separated)
# ORIGIN_EXTRA=https://<your-user>.github.io/<repo>,https://another-frontend.example.com

# Development helpers
ALLOW_LOCALHOST=true

# Port/host
PORT=3001
HOST=0.0.0.0

# Optional stamps
BUILD_STAMP=
VERSION=

# Optional: storage + legacy settings
# backblaze / s3-compatible
B2_KEY_ID=
B2_APP_KEY=
B2_BUCKET=
B2_REGION=
B2_S3_ENDPOINT=

# optional banner fallback
MAINTENANCE_MSG=

# foundations tuning
FOUNDATION_TARGET=0.8
FOUNDATION_MIN_PRACTICE=2
